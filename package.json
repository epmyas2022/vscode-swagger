{
  "name": "swaggercraft",
  "displayName": "swaggercraft",
  "description": "extension para practicar - autocomplete",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swaggercraft.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "swaggercraft.configInit",
        "title": "Config Init for Swagger"
      },
      {
        "command": "swaggercraft.loadConfigProyect",
        "title": "Load Config Proyect for Swagger"
      },

      {
        "command": "swaggercraft.onSaveConfig",
        "title": "On Save Config Build for Swagger"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "swagger-autogen": "^2.23.7",
    "typescript": "^5.3.3"
  }
}
